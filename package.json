		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"clean-webpack-plugin": "^0.1.19",
		"concurrently": "^3.6.0",
		"css-loader": "^0.28.11",
		"html-webpack-plugin": "^3.2.0",
		"nodemon": "^1.18.2",
		"style-loader": "^0.21.0",
		"webpack": "^4.12.0",
		"webpack-cli": "^3.0.8",
		"webpack-dev-server": "^3.1.4"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"bootstrap": "^4.1.2",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"firebase-admin": "^5.12.1",
